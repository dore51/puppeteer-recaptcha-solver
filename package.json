{
  "name": "@dore51/puppeteer-recaptcha-solver",
  "version": "1.0.0",
  "description": "ReCaptcha solver for puppeteer",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "example": "ts-node example/example.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dore51/puppeteer-recaptcha-solver.git"
  },
  "keywords": [
    "captcha",
    "captcha solver",
    "google captcha"
  ],
  "author": "dore51",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dore51/puppeteer-recaptcha-solver/issues"
  },
  "homepage": "https://github.com/dore51/puppeteer-recaptcha-solver#readme",
  "devDependencies": {
    "@types/jest": "^29.2.2",
    "jest": "^29.3.1",
    "prettier": "^2.7.1",
    "puppeteer": "^19.2.2",
    "puppeteer-screen-recorder": "^2.1.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "axios": "^0.27.2"
  },
  "files": [
    "lib/**/*"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  }
}
